{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "PackUI Config",
  "description": "Config schema for PackUI",
  "type": "object",
  "properties": {
    "customSongsFolder": {
      "description": "Path to the folder where the custom songs are stored",
      "type": "string"
    },
    "sources": {
      "description": "Array of download source urls",
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}
