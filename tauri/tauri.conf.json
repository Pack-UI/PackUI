{
	"$schema": "../node_modules/@tauri-apps/cli/schema.json",
	"build": {
		"beforeBuildCommand": "npm run build && npm run export",
		"beforeDevCommand": "npm run dev",
		"devPath": "http://localhost:3000",
		"distDir": "../out"
	},
	"package": {
		"productName": "packui",
		"version": "0.1.0"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"fs": {
				"all": true
			},
			"protocol": {
				"all": false,
				"asset": true,
				"assetScope": ["**/**"]
			},
			"dialog": {
				"all": false,
				"ask": false,
				"confirm": false,
				"message": false,
				"open": true,
				"save": false
			}
		},
		"bundle": {
			"active": true,
			"category": "DeveloperTool",
			"copyright": "",
			"deb": {
				"depends": []
			},
			"externalBin": [],
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "com.thijnmens.packui",
			"longDescription": "",
			"macOS": {
				"entitlements": null,
				"exceptionDomain": "",
				"frameworks": [],
				"providerShortName": null,
				"signingIdentity": null
			},
			"resources": [],
			"shortDescription": "",
			"targets": "all",
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"security": {
			"csp": "default-src 'self'; img-src 'self' asset: https://asset.localhost"
		},
		"updater": {
			"active": false
		},
		"windows": [
			{
				"fullscreen": false,
				"height": 800,
				"resizable": true,
				"title": "PackUI",
				"width": 1200
			}
		]
	}
}
